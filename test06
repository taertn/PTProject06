# for

for x in range (5) : # เริ่มที่ 0-4
    print(f'IoT {x}')
    
print('+++++++++++++++++++++')
for y in range (10) : # เริ่มที่ 0-9
    print(f'SAU {y}')
    
print('+++++++++++++++++++++')
for z in range (2,10) : # เริ่มที่ 2-9
    print(f'Hi {z}')
    
print('+++++++++++++++++++++')
for a in range (3,10,2) : #เริ่มที่ 3-9 / step +2
    print(f'Hey {a}')
    
print('+++++++++++++++++++++')
for b in 'Ateez present' :
    print(f'Hi {b}')